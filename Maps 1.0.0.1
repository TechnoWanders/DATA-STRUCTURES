#include<bits/stdc++.h>
using namespace std;
int main()
{
   multimap<int , string> mm;
   int i,n;
   string s;
   int value;
   cin>>n;
   for(i=0;i<n;i++)
   {
       cin>>s>>value;
       mm.insert({value,s});
   }
    map<int, string > :: iterator iit;
    iit = --mm.end();
   while(true)
   {
       auto &student_name = (*iit).second;
       int marks = (*iit).first;
        cout<<student_name<<" "<<marks<<endl;
       if(iit == mm.begin() ) break;
       iit--;
   }
}
